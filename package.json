{
  "name": "catan-board-generator",
  "version": "0.1.0",
  "description": "Settlers of Catan Project",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "predeploy": "cd client && npm run build",
    "deploy": "cd client && gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeKrummenacher/catan-board-generator.git"
  },
  "author": "Jake Krummenacher and Sam McGrath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JakeKrummenacher/catan-board-generator/issues"
  },
  "homepage": "https://github.com/JakeKrummenacher/catan-board-generator#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  }
}
